{
  "name": "parsebone",
  "description": "Wrapper around the Parse JS SDK that gives it the Backbone features it used to have.",
  "main": [
    "build/parsebone.js"
  ],
  "moduleType": [
    "globals",
    "amd",
    "es6"
  ],
  "keywords": [
    "date",
    "manipulation",
    "manipulate",
    "extended",
    "extend",
    "utilities",
    "utils"
  ],
  "authors": [
    "Luis Rodrigues (http://www.luisrodriguesweb.com)"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "configVars.js",
    "gulpfile.js",
    "karma.conf.js",
    "package.json",
    "npm-shrinkwrap.json",
    "node_modules",
    "tests",
    "src/**/*.spec.js",
    "gulp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrSlide/ParseBone.git"
  }
}
